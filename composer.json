{
  "name": "citizen63000/easy-api-jwt-authentication",
  "description": "Complete API bundle to create easily your API",
  "keywords": ["api", "rest"],
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "citizen63000"
    }
  ],
  "require": {
    "php": "^8.1",
    "citizen63000/easy-api-bundle": "dev-master|dev-php8|^3.0",
    "gesdinet/jwt-refresh-token-bundle": "^1.1"
  },
  "require-dev": {
    "citizen63000/easy-api-tests": "^2.0",
    "friendsofphp/php-cs-fixer": "@stable",
    "guzzlehttp/psr7": "^1.3",
    "php-http/guzzle6-adapter": "^1.1",
    "symfony/phpunit-bridge": "^3.0",
    "phpunit/phpunit": "6.*"
  },
  "config": {
    "discard-changes": true,
    "platform": {
      "php": "8.1"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "EasyApiJwtAuthentication\\": ""
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "extra": {
    "symfony": {
      "docker": false
    }
  }
}
